{
	"name": "figure-it-out-csv-export",
	"version": "1.1.0",
	"private": true,
	"description": "Difficult interview challenge: fix broken CSV export with streaming & backpressure (TypeScript)",
	"main": "dist/server.js",
	"scripts": {
		"start": "node --expose-gc --max-old-space-size=64 dist/server.js",
		"start:relaxed": "node --expose-gc --max-old-space-size=80 dist/server.js",
		"build": "tsc",
		"dev": "tsc -w",
		"test:slow": "curl --limit-rate 10k 'http://localhost:3000/residents/export.csv?count=1000000' -o /dev/null",
		"test:slow:small": "curl --limit-rate 10k 'http://localhost:3000/residents/export.csv?count=10000' -o /dev/null",
		"test:small": "curl 'http://localhost:3000/residents/export.csv?count=10000' -o /dev/null",
		"test:abort": "node -e \"console.log('Starting download; press Ctrl+C to abort...')\" && curl 'http://localhost:3000/residents/export.csv?count=1000000'",
		"test:massive": "curl 'http://localhost:3000/residents/export.csv?count=5000000' -o ./massive-output.csv"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"license": "UNLICENSED",
	"dependencies": {
		"tsc": "^2.0.4",
		"typescript": "^5.9.3"
	},
	"devDependencies": {
		"@types/node": "^25.0.9"
	}
}
